    "@types/react-dom": "16.8.5",
    "@types/react-test-renderer": "16.9.0",
    "@types/styled-components": "4.1.18",
    "ava": "2.2.0",
    "enzyme": "3.10.0",
    "enzyme-adapter-react-16": "1.14.0",
    "jasmine-core": "3.4.0",
    "jsdom": "15.1.1",
    "karma": "4.2.0",
    "karma-chrome-launcher": "3.0.0",
    "karma-firefox-launcher": "1.2.0",
    "karma-ie-launcher": "1.0.0",
