{
  "name": "@ngineer/testing-utils",
  "version": "1.3.1",
  "description": "A collection testing tools and utilities",
  "homepage": "https://pixelass.github.io/ngineer",
  "bugs": {
    "url": "https://github.com/pixelass/ngineer/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pixelass/ngineer.git"
  },
  "license": "MIT",
  "author": "Gregor Adams <greg@pixelass.com>",
  "files": [
    "dist",
    "index.js",
    "index.d.ts"
  ],
  "main": "index.js",
  "module": "dist/esm/index.js",
  "types": "index.d.ts",
  "directories": {
    "lib": "dist",
    "doc": "doc",
    "test": "test"
  },
  "scripts": {
    "build": "ngineer -rp",
    "clean": "ngineer clean",
    "format": "ngineer format 'src/**/*.{ts,tsx}' '**/*.md'",
    "lint": "ngineer lint 'src/**/*.{ts,tsx}'",
    "prebuild": "yarn clean",
    "watch": "ngineer -rw"
